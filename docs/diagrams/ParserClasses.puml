@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor LOGIC_COLOR_T4
skinparam classBackgroundColor LOGIC_COLOR

Class "{abstract}\nCommand" as Command
Class ResetCommand

package "Parser classes"{
    Class "<<interface>>\nParser" as Parser
    Class AddressBookParser
    Class ResetCommandParser
    Class CliSyntax
    Class ParserUtil
    Class ArgumentMultimap
    Class ArgumentTokenizer
    Class Prefix
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> AddressBookParser

' Parser relationships
AddressBookParser .down.> ResetCommandParser : <<create>>
ResetCommandParser .up.|> Parser
ResetCommandParser ..> ResetCommand : <<create>>

' Utility class relations
AddressBookParser ..> Command : <<use>>
ResetCommandParser ..> ArgumentMultimap
ResetCommandParser ..> ArgumentTokenizer
ArgumentTokenizer .left.> ArgumentMultimap
ResetCommandParser ..> CliSyntax
CliSyntax ..> Prefix
ResetCommandParser ..> ParserUtil
ParserUtil .down.> Prefix
ArgumentTokenizer .down.> Prefix

' Command hierarchy
ResetCommand -up-|> Command
@enduml
